    "def linearSearch(arr, search_element, n):\n",
    "    for i in range(n):\n",
    "        if arr[i] == search_element:\n",
    "            return i\n",
    "    return -1\n",
    "arr = [4,3,56,34,22]\n",
    "search_element = 34\n",
    "n = len(arr)\n",
    "result = linearSearch(arr, search_element, n)\n",
    "\n",
    "\n",
    "print(result)"
   ]
