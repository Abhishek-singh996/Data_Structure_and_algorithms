    "def DAC_Power(a,n):\n",
    "    if n==1:\n",
    "        return a\n",
    "    else:\n",
    "        mid = n//2\n",
    "        b = DAC_Power(a,mid)\n",
    "        c = b*b\n",
    "        if n%2==0:\n",
    "            return c\n",
    "        else:\n",
    "            return (c*a)\n",
    "a = 2\n",
    "n = 65\n",
    "result = DAC_Power(a,n)\n",
    "print(\"result is:\",result)"
   ]
